<table class="table table-dark">
<thead>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Password</th>
        <th>City</th>
    </tr>
</thead>
<tbody>
    <tr>
        <td>@Model.id</td>
        <td>@Model.name</td>
        <td>@Model.password</td>
        <td>@Model.city</td>
    </tr>
</tbody>
</table>